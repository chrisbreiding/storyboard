<li {{bind-attr class="current_state story_type"}}>
  {{story_icon story_type estimate}}
  {{story_state current_state}}
  <ul class="labels">
    {{#each labels}}
      <li>{{this}}</li>
    {{/each}}
  </ul>
  <div class="main">
    {{markdown name}}
    <ul class="owners">
      {{#each owners}}
        <li>{{this}}</li>
      {{/each}}
    </ul>
  </div>
</li>
